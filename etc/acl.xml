<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Theiconnz_Campaigns::configuration" title="Campaign Configuration" sortOrder="10"/>
                            <resource id="Theiconnz_Campaigns::campaign" title="Campaign Campaigns" sortOrder="10"/>
                            <resource id="Theiconnz_Campaigns::save" title="Campaign Campaign Save" sortOrder="10"/>
                            <resource id="Theiconnz_Campaigns::results" title="Campaigns Results" sortOrder="10"/>
                            <resource id="Theiconnz_Campaigns::resultssave" title="Campaign Results Save" sortOrder="10"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
